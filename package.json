{
  "name": "mapbox-style-downloader",
  "version": "2.0.0",
  "description": "Download Mapbox styles, sprites, glyphs (fonts) and tiles for offline usage",
  "main": "index.js",
  "bin": {
    "mapbox-style": "./bin/mapbox-style.js"
  },
  "scripts": {
    "test": "npm run dependency-check",
    "dependency-check": "dependency-check package.json && dependency-check package.json --unused --no-dev"
  },
  "keywords": [
    "mapbox",
    "offline",
    "tile",
    "map",
    "vector",
    "tiles"
  ],
  "author": "Gregor MacLennan",
  "license": "MIT",
  "dependencies": {
    "@mapbox/tilejson": "^1.1.0",
    "@mapbox/tilelive": "^6.0.0",
    "d3-queue": "^3.0.7",
    "debug": "^3.1.0",
    "ecstatic": "^3.2.0",
    "fs-extra": "^5.0.0",
    "getport": "^0.1.0",
    "mkdirp": "^0.5.1",
    "object-assign": "^4.1.1",
    "pump": "^3.0.0",
    "request": "^2.85.0",
    "sanitize-filename": "^1.6.1",
    "tilelive-file": "0.0.3",
    "tilelive-streaming": "^0.7.7",
    "traverse": "^0.6.6",
    "yargs-parser": "^10.0.0"
  },
  "devDependencies": {
    "dependency-check": "^3.1.0"
  }
}
